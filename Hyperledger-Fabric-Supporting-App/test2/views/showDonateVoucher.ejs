<body ng-app="application" ng-controller="appController" id="body">

<!-- Header Start -->
<%- include("header") %>

<section class="page-title bg-2">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="block">
          <h1>Hyperledger Fabric Supporting Application.</h1><br>
          <p>기부한 바우처 내역 조회</p>
        </div>
      </div>
    </div>
  </div>
</section>

<section class="portfolio-work">
  <div class="container">
    <div class="row">
      <!--현재 필터: <%=filter%>
      <form method="post" action="/supporter/filter">
        <select name="kind">
            <option value="all">전체</option>
            <option value="y">사용 완료</option>
            <option value="n">사용 가능</option>
        </select>
        <input id="edit" class="btn btn-primary" type="submit" value="필터">
      </form>-->

    <table class="table" align="center">
        <th>
          <td>총 블록수</td>
        </th>
        <th>
            <td><%=data.amount%></td>
        </th>
    </table>
    <table class="table" align="center">
      <tr>
        <th>블록 수</th>
        <th>+/-</th>
        <th>거래 시간</th>
        <th>트랜잭션 ID</th>
      </tr>
      <%for (var i = data2.length-1; i >=0; i--) { 
        db = data2[i];
        if(db.Value.giveget>0){
      %>
        <tr>
            <td><%=db.Value.amount%></td>
            <td>+<%=db.Value.giveget%></td>
            <td><%=db.Timestamp%></td>
            <td><%=db.TxId%></td>
        </tr>
     <%}else{%>
      <tr>
          <td><%=db.Value.amount%></td>
          <td><%=db.Value.giveget%></td>
          <td><%=db.Timestamp%></td>
          <td><%=db.TxId%></td>
      </tr>
      <%}}%>
     </table>
<!-- footer Start -->
<footer class="footer">
<%- include("footer") %>
</footer>
