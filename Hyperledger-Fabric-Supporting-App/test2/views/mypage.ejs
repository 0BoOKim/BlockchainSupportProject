<html class="no-js"> <!--<![endif]-->
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <meta name="description" content="Aviato E-Commerce Template">
  
  <meta name="author" content="Themefisher.com">

  <title>Airspace | Creative Agency Bootstrap template</title>

  <!-- Mobile Specific Meta-->
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Favicon -->
  <link rel="shortcut icon" type="image/x-icon" href="img/favicon.png" />
  <!-- bootstrap.min css -->
  <link rel="stylesheet" href="plugins/bootstrap/css/bootstrap.min.css">
  <!-- Ionic Icon Css -->
  <link rel="stylesheet" href="plugins/Ionicons/css/ionicons.min.css">
  <!-- animate.css -->
  <link rel="stylesheet" href="plugins/animate-css/animate.css">
  <!-- Magnify Popup -->
  <link rel="stylesheet" href="plugins/magnific-popup/dist/magnific-popup.css">
  <!-- Owl Carousel CSS -->
  <link rel="stylesheet" href="plugins/slick-carousel/slick/slick.css">
  <link rel="stylesheet" href="plugins/slick-carousel/slick/slick-theme.css">
  
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.3/angular.min.js"></script>

  <!-- Main Stylesheet -->
  <link rel="stylesheet" href="css/style.css">
</head>

<body ng-app="application" ng-controller="appController" id="body">
<!-- Header Start -->
<%- include("header") %><!-- header close -->
<section class="page-title bg-2">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="block">
          <h1>Hyperledger Fabric Supporting Application</h1>
          <p>mypage</p>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- contact form start -->
<section class="contact-form">
    <div class="container"> 
      <div class="form-group">
        <label>내 정보</label>
        <table class="table" align="center">
          <tr>
            <td width="30%" border-right="1px solid black">이름</td>
            <td><%=session.Name%></td>
            훙<%=session.auth%>
          </tr>
          <tr>
            <td width="30%" border-right="1px solid black">해당 그룹</td>
              <%if(session.auth==0){%>
                <td>후원자</td>
              <%}else if(session.auth==1){%>
                <td>피후원자</td>
              <%}else if(session.auth==2){%>
                <td>공공기관</td>
              <%}else {%>
                <td>후원기관</td>
              <%}%>
            </tr>
        </table>
      </div><br>
      <div class="form-group">
        <label>결제용 비밀번호 등록</label>
        <a id="search"class="btn btn-default" href="#" role="button">비밀번호 등록</a>
      </div><br>
      <div class="form-group">
        <label>내 정보 수정</label><br>
        <%if (session.auth == 0){%>
          <form class="form-register" method="post" action="/supporter/supp_personal_info">
          주민번호를 입력해 주세요 <input type = "text" class="form-control" id="id" name="id">
          비밀번호를 입력해 주세요 <input type="password" class="form-control" id="password" name="password">
          <input id="edit" class="btn btn-default" type="submit" value="확인">
        </form>
      </div>
        <%}
         else if (session.auth == 1){%>
          <form class="form-register" method="post" action="/recipient/reci_personal_info">
            주민번호를 입력해 주세요 <input type = "text" class="form-control" id="id" name="id">
            비밀번호를 입력해 주세요 <input type="password" class="form-control" id="password" name="password">
            <input id="edit" class="btn btn-default" type="submit" value="확인">
          </form>
        </div>
          <%}%><br>
      
          <div class="form-group">
            <label>비밀번호 수정</label><br>
            <%if (session.auth == 0){%>
              <form class="form-register" method="post" action="/supporter/supp_pass_info">
              주민번호를 입력해 주세요 <input type = "text" class="form-control" id="id" name="id">
              비밀번호를 입력해 주세요 <input type="password" class="form-control" id="password" name="password">
              <input id="edit" class="btn btn-default" type="submit" value="확인">
            </form>
          </div>
            <%}
             else {%>
              <form class="form-register" method="post" action="">
                주민번호를 입력해 주세요 <input type = "text" class="form-control" id="id" name="id">
                비밀번호를 입력해 주세요 <input type="password" class="form-control" id="password" name="password">
                <input id="edit" class="btn btn-default" type="submit" value="확인">
              </form>
            </div>
              <%}%>
      

<!-- footer Start -->
<footer class="footer">
<%- include("footer") %>
</footer>
    <script src="https://code.jquery.com/jquery-git.min.js"></script>
    <!-- Bootstrap 3.1 -->
    <script src="plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- Owl Carousel -->
    <script src="plugins/slick-carousel/slick/slick.min.js"></script>
    <!--  -->
    <script src="plugins/magnific-popup/dist/jquery.magnific-popup.min.js"></script>
    <!-- Mixit Up JS -->
    <script src="plugins/mixitup/dist/mixitup.min.js"></script>
    <!-- <script src="plugins/count-down/jquery.lwtCountdown-1.0.js"></script> -->
    <script src="plugins/SyoTimer/build/jquery.syotimer.min.js"></script>


    <!-- Form Validator -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery.form/3.32/jquery.form.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.11.1/jquery.validate.min.js"></script>

    <script src="js/script.js"></script>
    <script type="text/javascript" src="app.js"></script>
  </body>
  </html>
