<body ng-app="application" ng-controller="appController" id="body">
<!-- Header Start -->
<%- include("header") %><!-- header close -->
<section class="page-title bg-2">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="block">
          <h1>개인 후원 시스템</h1>
          <p>내 정보 수정</p>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- contact form start -->
<section class="portfolio-work">
  <div class="container">
    <div class="row">
      <table class="table" align="center">
          <form method="post" action="/supporter/changeAl">
          <tr>
            <td width="30%" border-right="1px solid black">이름</td>
            <td><%=data.name%></td>
          </tr>
          <tr>
            <td width="30%" border-right="1px solid black">주민등록번호</td>
            <td><%=data.id%></td>
          </tr>
          <tr>
          <tr>
              <td width="30%" border-right="1px solid black">핸드폰 번호</td>
              <td><input type="text" name="phoneNum" value=<%=data.phoneNum%>></td>
          </tr>
          <tr>
              <td width="30%" border-right="1px solid black">이메일</td>
              <td><%=data.email%></td>
          </tr>
          <tr>
              <td width="30%" border-right="1px solid black">주소</td>
              <td><input type="text" name="address" value=<%=data.address%>></td>
          </tr>
          <tr><td colspan="2"><input id="send" class="btn btn-default" type="submit" value="수정"></td></tr>
        </table>
    </form>

<!-- footer Start -->
<footer class="footer">
<%- include("footer") %>
</footer>
