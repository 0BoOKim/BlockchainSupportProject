<body ng-app="application" ng-controller="appController" id="body">
<!-- Header Start -->
<%- include("header") %><!-- header close -->
<section class="page-title bg-2">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="block">
          <h1>Hyperledger Fabric Supporting Application</h1>
          <p>Login / Logout</p>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- contact form start -->
<section class="contact-form">
    <div class="container"> 
      <div class="form-group">
        <label>내 정보</label>
        <table class="table" align="center">
          <tr>
            <td>이름</td>
            <td><%=session.Name%></td>
          </tr>
          <tr>
            <td>해당 그룹</td>
              <%if(session.auth==0){%>
                <td>후원자</td>
              <%}
              else if(session.auth==1){%>
                <td>피후원자</td>
              <%}%>
          </tr>
        </table>
      </div>
      <div class="form-group">
        <label>내 정보 수정</label><br>
        <%if (session.auth == 0){%>
          <form class="form-register" method="post" action="/supporter/supp_personal_info">
          주민번호를 입력해 주세요 <input type = "text" class="form-control" id="id" name="id">
          비밀번호를 입력해 주세요 <input type="password" class="form-control" id="password" name="password">
          <input id="edit" class="btn btn-primary" type="submit" value="확인">
        </form>
        <%}
         else {%>
          <form class="form-register" method="post" action="/recipient/reci_personal_info">
            주민번호를 입력해 주세요 <input type = "text" class="form-control" id="id" name="id">
            비밀번호를 입력해 주세요 <input type="password" class="form-control" id="password" name="password">
            <input id="edit" class="btn btn-primary" type="submit" value="확인">
          </form>
          <%}%>
      
       
      <!-- 로그인 한 유저의 주민번호 정보로 바로 조회하여 개인 정보들을 띄워주는 것으로 수정 필요-->
      <div class="form-group">
        <label>내 정보 조회(피후원자)</label><br>
        
        주민등록번호: <input id="createName" class="form-control" type="text" placeholder="Ex: 3" ng-model="recipient_id">
        <input id="querySubmit" type="submit" value="Query" class="btn btn-primary" ng-click="queryRecipient()">
      </div>

      <table id="query_recipient" class="table" align="center">

        <tr>
          <th>이름</th>
          <th>주민등록번호</th>
          <th>이메일</th>
          <th>비밀번호</th>
          <th>주소</th>
          <th>전화번호</th>
          <th>신청 사유</th>
          <th>상태</th>
        </tr>

        <tr>
          <td>{{query_recipient.name}}</td>
          <td>{{query_recipient.id}}</td>
          <td>{{query_recipient.email}}</td>
          <td>{{query_recipient.password}}</td>
          <td>{{query_recipient.address}}</td>
          <td>{{query_recipient.phoneNum}}</td>
          <td>{{query_recipient.story}}</td>
          <td>{{query_recipient.status}}</td>
        </tr>
      </table>
<!--    <div class="form-group">
        <label>Create Tuna Record</label>
        <h5 style="color:green;margin-bottom:2%" id="success_create">Success! Tx ID: {{create_tuna}}</h5>
        <br>
        Enter catch id: <input class="form-control" type="text" placeholder="Ex: 11" ng-model="tuna.id">
        Enter name of vessel: <input class="form-control" type="text" placeholder="Ex: 0239L" ng-model="tuna.vessel">
